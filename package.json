{
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "bunchee ./src/index.ts",
    "watch": "bunchee ./src/index.ts -w",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "exports": {
    "require": "dist/index.cjs",
    "import": "dist/index.mjs",
    "module": "dist/index.esm.js"
  },
  "name": "wizard-state",
  "author": "Dominik Garcia Bertapelle",
  "size-limit": [
    {
      "path": "dist/wizard-state.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/wizard-state.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^8.2.4",
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "bunchee": "^2.2.1",
    "husky": "^8.0.3",
    "prettier": "^2.8.7",
    "size-limit": "^8.2.4",
    "tslib": "^2.5.0",
    "typescript": "5.0.2",
    "vitest": "^0.29.8"
  },
  "dependencies": {
    "jotai": "^2.0.3",
    "jotai-xstate": "^0.3.0",
    "xstate": "^4.37.1"
  }
}
